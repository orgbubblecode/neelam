@model MailChimp.Net.Models.List
@{
    ViewBag.Title = "View List";
}

<h2>View List</h2>

<table class="table table-striped">
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Created On</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>@Model.Id</td>
            <td>@Model.Name</td>
            <td>@Model.DateCreated</td>
        </tr>
    </tbody>
</table>